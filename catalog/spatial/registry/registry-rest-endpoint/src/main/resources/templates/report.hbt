<!DOCTYPE html>
<html>
<head>
    <meta content="text/html; charset=UTF-8" http-equiv="content-type"/>
    <!-- styles -->
    <link href="/services/registries/assets/css/report.css" rel="stylesheet"/>
    <link href="/services/registries/assets/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="/services/registries/assets/css/bootstrap-theme.min.css" rel="stylesheet"/>
</head>
<body>
{{#each ExtrinsicObject}}
    {{#ifeq objectType 'urn:registry:federation:node'}}
            <h1 class="NodeName">{{Name}}</h1>
            <table class="table table-striped ">
                <tbody>
                {{#each this}}
                    {{#ifeq @key 'Slot'}}
                        {{#each this}}
                            <tr class="NodeValuePair">
                                <td class="column">
                                    <strong class="NodeKey">{{name}}</strong>
                                </td>
                                <td class="NodeValue">{{join value ", "}}</td>
                            </tr>
                        {{/each}}
                    {{else}}
                        {{#ifeq @key 'Classification'}}
                            {{#each this}}
                                {{#each this}}
                                    <tr>
                                        <td class="column">
                                            <strong>{{@key}}</strong>
                                        </td>
                                        <td>{{join this " "}}</td>
                                    </tr>
                                {{/each}}
                            {{/each}}
                        {{else}}
                            <tr class="NodeValuePair">
                                <td class="column">
                                    <strong class="NodeKey">{{@key}}</strong>
                                </td>
                                <td class="NodeValue">{{this}}</td>
                            </tr>
                        {{/ifeq}}
                    {{/ifeq}}
                {{/each}}
                </tbody>
            </table>
    {{/ifeq}}
{{/each}}
    <h2>Services</h2>
    {{#each Service}}
        <h3 class="ServiceName">{{Name}}</h3>
        <table class="table table-striped">
            <tbody>
            {{#each this}}
                {{#ifeq @key 'ServiceBinding'}}
                {{else}}
                    <tr class="ServiceValuePair">
                        <td class="column">
                            <strong class="ServiceKey">{{@key}}</strong>
                        </td>
                        <td class="ServiceValue">{{this}}</td>
                    </tr>
                {{/ifeq}}
            {{/each}}
            </tbody>
        </table>
        {{#each this.ServiceBinding}}
            <h4>{{Name}}</h4>
            <table class="table table-striped">
                <tbody>
                {{#each this.Slot}}
                    <tr class="ServiceValuePair">
                        <td class="column">
                            <strong class="ServiceKey">{{name}}</strong>
                        </td>
                        <td class="ServiceValue">{{join value " "}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        {{/each}}
        {{#each this.Bindings}}
            <h4>{{@key}}</h4>
            <table class="table table-striped ">
                <tbody>
                {{#each this}}
                    <tr class="ServiceValuePair">
                        <td class="column">
                            <strong class="ServiceKey">{{@key}}</strong>
                        </td>
                        <td class="ServiceValue">{{this}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        {{/each}} {{/each}}
<h2>Organizations</h2>
{{#each Organization}}
    <h3 class="Name">{{Name}}</h3>
    {{#each this}}
        {{#ifeq @key 'TelephoneNumber'}}
            <h4>Phone Numbers</h4>
            {{#each this}}
                <table class="table table-striped ">
                    <tr>
                        <td class="Phone">{{#if areaCode}}({{areaCode}})
                        {{/if}}{{#if number}}{{number}}{{/if}}
                            {{#if extension}}ext. {{extension}}{{/if}}</td>
                    </tr>
                </table>
            {{/each}}
        {{else}}
            {{#ifeq @key 'EmailAddress'}}
                <h4>Email Addresses</h4>
                {{#each this}}
                    <table class="table table-striped ">
                        <tr>
                            <td class="Email">{{address}}</td>
                        </tr>
                    </table>
                {{/each}}
            {{else}}
                {{#ifeq @key 'Address'}}
                    <h4>Addresses</h4>
                    {{#each this}}
                        <table class="table table-striped ">
                            <tr>
                                <td class="Address">{{street}} {{#if city}}{{city}},{{/if}}
                                    {{stateOrProvince}} {{postalCode}}</td>
                            </tr>
                        </table>
                    {{/each}}
                {{else}}
                    {{#ifeq @key 'Name'}}
                    {{else}}
                        {{#ifeq @key 'Slot'}}
                            {{#each this}}
                                <h4>{{name}}</h4>
                                <table class="table table-striped">
                                    <tr>
                                        <td class="Slot">{{join value ", "}}</td>
                                    </tr>
                                </table>
                            {{/each}}
                        {{else}}
                            {{#ifeq @key 'Classification'}}
                                {{#each this}}
                                    {{#each this}}
                                        <h4>{{@key}}</h4>
                                        <table class="table table-striped">
                                            <tr>
                                                <td class="Classifications">{{join this " "}}</td>
                                            </tr>
                                        </table>
                                    {{/each}}
                                {{/each}}
                            {{else}}
                                <div class="ValuePair">
                                    <h4 class="key">{{@key}}</h4>
                                    <table class="table table-striped ">
                                        <tr>
                                            <td class="value">{{this}}</td>
                                        </tr>
                                    </table>
                                </div>
                            {{/ifeq}}
                        {{/ifeq}}
                    {{/ifeq}}
                {{/ifeq}}
            {{/ifeq}}
        {{/ifeq}}
    {{/each}}
{{/each}}
    <h2>Contacts</h2>
    {{#each Person}}
        <h3 class="ContactName">{{PersonName.firstName}} {{PersonName.lastName}}</h3>
        {{#each this}}
            {{#ifeq @key 'TelephoneNumber'}}
                <h4>Phone Numbers</h4>
                {{#each this}}
                    <table class="table table-striped ">
                        <tr>
                            <td class="ContactPhone">{{#if areaCode}}({{areaCode}})
                            {{/if}}{{#if number}}{{number}}{{/if}}
                                {{#if extension}}ext. {{extension}}{{/if}}</td>
                        </tr>
                    </table>
                {{/each}}
            {{else}}
                {{#ifeq @key 'EmailAddress'}}
                    <h4>Email Addresses</h4>
                    {{#each this}}
                        <table class="table table-striped ">
                            <tr>
                                <td class="ContactEmail">{{address}}</td>
                            </tr>
                        </table>
                    {{/each}}
                {{else}}
                    {{#ifeq @key 'Address'}}
                        <h4>Addresses</h4>
                        {{#each this}}
                            <table class="table table-striped ">
                                <tr>
                                    <td class="ContactAddress">{{street}} {{#if city}}{{city}}
                                        ,{{/if}}
                                        {{stateOrProvince}} {{postalCode}}</td>
                                </tr>
                            </table>
                        {{/each}}
                    {{else}}
                        {{#ifeq @key 'PersonName'}}
                        {{else}}
                            {{#ifeq @key 'Classification'}}
                                {{#each this}}
                                    {{#each this}}
                                        <h4>{{@key}}</h4>
                                        <table class="table table-striped">
                                            <tr>
                                                <td>{{join this " "}}</td>
                                            </tr>
                                        </table>
                                    {{/each}}
                                {{/each}}
                            {{else}}
                                {{#ifeq @key 'Name'}}
                                    <h4>Title</h4>
                                {{else}}
                                <div class="ContactValuePair">
                                    <h4 class="ContactKey">{{@key}}</h4>
                                {{/ifeq}}
                                <table class="table table-striped ">
                                    <tr>
                                        <td class="ContactValue">{{this}}</td>
                                    </tr>
                                </table>
                            </div>
                            {{/ifeq}}
                        {{/ifeq}}
                    {{/ifeq}}
                {{/ifeq}}
            {{/ifeq}}
        {{/each}}
    {{/each}}
    <h2>Collections</h2>
    {{#each ExtrinsicObject}}
        {{#ifeq objectType 'urn:registry:content:collection'}}
            <h3>{{Name}}</h3>
            <table class="table table-striped ">
                <tbody>
                {{#each this}}
                    {{#ifeq @key 'Slot'}}
                        {{#each this}}
                            <tr class="CollectionValuePair">
                                <td class="column">
                                    <strong class="CollectionKey">{{name}}</strong>
                                </td>
                                <td class="CollectionValue">{{join value " "}}</td>
                            </tr>
                        {{/each}}
                    {{else}}
                        <tr class="CollectionValuePair">
                            <td class="column">
                                <strong class="CollectionKey">{{@key}}</strong>
                            </td>
                            <td class="CollectionValue">{{this}}</td>
                        </tr>
                    {{/ifeq}}
                {{/each}}
                </tbody>
            </table>
        {{/ifeq}}
    {{/each}}
</body>
</html>