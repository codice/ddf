<schema xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:test="http://codice.org/test"
  targetNamespace="http://codice.org/test">
  <complexType name="CustomFeatureType">
    <sequence>
      <element name="TheProperty">
        <simpleType>
          <restriction base="xsd:string">
            <minLength value="1"/>
            <minLength value="10"/>
          </restriction>
        </simpleType>
      </element>
    </sequence>
  </complexType>
  <element name="FeatureTypeName" type="test:CustomFeatureType"/>
</schema>
