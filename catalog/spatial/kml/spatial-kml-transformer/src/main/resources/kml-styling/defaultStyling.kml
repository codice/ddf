<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
	<Document>
		<Style id="bluenormal">
<!-- Update when Icons become available 
      <IconStyle>
        <Icon>
          <href>/icons?iconId=bomber-2.png</href>
        </Icon>
        <scale>0.7</scale>
      </IconStyle>
      -->
			<LabelStyle>
				<scale>0.0</scale>
			</LabelStyle>
			<LineStyle>
				<color>33ff0000</color>
				<width>3.0</width>
			</LineStyle>
			<PolyStyle>
				<color>33ff0000</color>
				<fill>1</fill>
			</PolyStyle>
			<BalloonStyle>
				<text><![CDATA[<h3><b>$[name]</b></h3><table><tr><td width="400">$[description]</td></tr></table>]]></text>
			</BalloonStyle>
		</Style>
		<Style id="bluehighlight">
    <!-- Update when Icons become available 
      <IconStyle>
        <Icon>
          <href>/icons?iconId=bomber-2.png</href>
        </Icon>
        <scale>1.0</scale>
      </IconStyle>
      -->
			<LabelStyle>
				<scale>1.0</scale>
			</LabelStyle>
			<LineStyle>
				<color>99ff0000</color>
				<width>6.0</width>
			</LineStyle>
			<PolyStyle>
				<color>99ff0000</color>
				<fill>1</fill>
			</PolyStyle>
			<BalloonStyle>
				<text><![CDATA[<h3><b>$[name]</b></h3><table><tr><td width="400">$[description]</td></tr></table>]]></text>
			</BalloonStyle>
		</Style>
		<StyleMap id="default">
			<Pair>
				<key>normal</key>
				<styleUrl>#bluenormal</styleUrl>
			</Pair>
			<Pair>
				<key>highlight</key>
				<styleUrl>#bluehighlight</styleUrl>
			</Pair>
		</StyleMap>
	</Document>
</kml>