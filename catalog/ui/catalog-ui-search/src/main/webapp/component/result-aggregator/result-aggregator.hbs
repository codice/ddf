<div class="resultAggregator-result">
    <div class="resultAggregator-resultTitle">
        <span>
            Title
        </span>
    </div>
    <div class="resultAggregator-resultSource">
        <span>
            Source
        </span>
    </div>
    <div class="resultAggregator-resultLastModified">
        <span>
            Last Modified
        </span>
    </div>
</div>
<div class="resultAggregator-list is-list">
    {{#each this}}
        <div class="resultAggregator-result is-button" data-metacardId="{{id}}" data-queryId="{{queryId}}">
            <div class="resultAggregator-resultTitle">
                <span>
                    {{properties.title}}
                </span>
            </div>
            <div class="resultAggregator-resultSource">
                <span>
                    {{properties.source-id}}
                </span>
            </div>
            <div class="resultAggregator-resultLastModified">
                <span>
                    {{properties.modified}}
                </span>
            </div>
        </div>
    {{/each}}
</div>