                <fes:Literal>true</fes:Literal>
                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
        <fes:PropertyIsEqualTo matchAction="ANY" matchCase="false">
            <fes:ValueReference>contact.publisher-name</fes:ValueReference>
