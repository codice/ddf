                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
        <fes:PropertyIsEqualTo matchAction="ANY" matchCase="false">
            <fes:ValueReference>contact.contributor-name</fes:ValueReference>
            <fes:Function name="forms.function.1">
                <fes:Literal/>
                <fes:Literal>my-id-same-source</fes:Literal>
                <fes:Literal>true</fes:Literal>
                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
    </fes:Or>
</fes:Filter>