                        </goals>
                        <configuration>
                            <resourcesDir>${project.build.directory}/resources</resourcesDir>
                            <featuresFile>${basedir}/target/classes/features.xml</featuresFile>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>
