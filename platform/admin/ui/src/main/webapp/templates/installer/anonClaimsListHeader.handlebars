 <div class='{{id}} metatype control-group'>
     <label class='control-label' for='{{id}}'>Add Attribute</label>
     <div class="btn-group">
         <a href="#" class="fa fa-plus-square fa-lg plus-button"></a>
     </div>
     <div class="row-container-div">
         <div class="claim-warn-column">
             {{#if showWarning}}
                 <a href='#' class='claims-warning'>
                     <i class="fa fa-exclamation-triangle fa-lg"></i>
                 </a>
             {{/if}}
         </div>
         <div class="claim-name-column claim-column-header">
             <label class='control-label claim-column-label'>Attribute Name</label>
         </div>
         <div class="claim-value-column claim-column-header">
             <label class='control-label claim-column-label'>Attribute Value</label>
         </div>
         <div class="claim-remove-column">
         </div>
     </div>
     <div id='listItems'>

     </div>

</div>
