<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
            <h4 class="modal-title">Update Application - {{displayName}}</h4>
        </div>
        <div class="modal-body">
            <form class="fileupload" >
                <span class="btn btn-info fileinput-button">
                    <i class="glyphicon glyphicon-plus"></i>
                    <span>Select Application File...</span>
                    <input type="file" name="files[]">
                </span>

                <div class="file-upload-region"></div>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary updateAppClose display-none" data-dismiss="modal" aria-hidden="true">Ok</button>
            <button type="button" class="btn btn-default updateAppCancel" data-dismiss="modal" aria-hidden="true">Cancel</button>
        </div>
    </div>
</div>