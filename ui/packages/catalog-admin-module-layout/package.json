{
  "name": "defaul-layout",
  "author": "Codice",
  "description": "The golden layouts default layout admin ui module.",
  "version": "1.0.0",
  "main": "index.js",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/codice/ddf.git"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  },
  "mocha": {
    "hooks": "mocha-phantomjs-istanbul",
    "coverageFile":"target/coverage.json"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --open",
    "start:test": "webpack-dev-server --progress --open --env=test",
    "pretest": "standard ./src/**/*.js && webpack --bail --env=ci",
    "test": "ace test target/ci/index.html",
    "posttest": "istanbul check-coverage",
    "coverage:report": "istanbul report html && opn ./target/coverage/index.html",
    "format": "standard --fix ./src/**/*.js",
    "build": "webpack --bail --progress && ace package",
    "build:stat": "webpack --json --env=production | webpack-bundle-size-analyzer",
    "clean": "ace clean"
  },
  "peerDependencies": {
    "react": "^15.1.0",
    "redux": "^3.5.2"
  },
  "dependencies": {
    "es6-promise": "4.0.5",
    "flexbox-react": "4.3.1",
    "immutable": "3.8.1",
    "material-ui": "0.17.4",
    "react": "15.4.2",
    "react-ace": "4.3.0",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-immutable": "3.0.11",
    "redux-thunk": "2.2.0",
    "golden-layout": "1.5.8",
    "font-awesome": "4.7.0",
    "react-fontawesome": "1.6.1",
    "validator": "7.0.0",
    "whatwg-fetch": "2.0.2",
    "traverse": "0.6.6"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "4.0.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "chai": "3.5.0",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "glob": "7.1.1",
    "html-webpack-plugin": "2.28.0",
    "less-loader": "2.2.3",
    "less-plugin-clean-css": "1.5.1",
    "istanbul": "0.4.5",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "mocha-loader": "1.1.0",
    "mocha-phantomjs-istanbul": "0.0.2",
    "opn-cli": "3.1.0",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "stack-source-map": "1.0.6",
    "standard": "8.6.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-bundle-size-analyzer": "2.5.0",
    "webpack-dev-server": "2.3.0",
    "webpack-merge": "2.6.1",
    "ace": "*"
  },
  "files": [
    "target/webapp",
    "src/main/resources"
  ],
  "context-path": "/admin/default-layout"
}
