{
  "name": "sources-module",
  "author": "Codice",
  "description": "The Catalog Admin Sources Module",
  "version": "2.5.0",
  "license": "LGPL-3.0",
  "keywords": [
    "backbone",
    "express"
  ],
  "main": "src/main/webapp/main.js",
  "scripts": {
