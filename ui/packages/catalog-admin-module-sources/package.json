    "url": "https://github.com/codice/ddf.git"
  },
  "engines": {
    "node": ">=0.10.5"
  },
  "devDependencies": {
    "ace": "*"
  },
  "dependencies": {
    "backbone-associations": "0.6.2",
    "backbone.marionette": "1.8.8",
    "backbone.modelbinder": "1.1.0",
    "backbone-poller": "1.1.3",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "lodash": "4.17.4"
  },
  "alias": {
    "icanhaz": "./src/main/webapp/js/ich",
    "backboneassociation": "backbone-associations",
    "marionette": "backbone.marionette",
    "modelbinder": "backbone.modelbinder",
    "poller": "backbone-poller"
  },
  "files": [
    "src/main/webapp",
