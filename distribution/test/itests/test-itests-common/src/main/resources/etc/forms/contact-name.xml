<fes:Filter xmlns:fes="http://www.opengis.net/fes/2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.opengis.net/fes/2.0 http://schemas.opengis.net/filter/2.0/filterAll.xsd">
    <fes:Or>
        <fes:PropertyIsEqualTo matchAction="ANY" matchCase="false">
            <fes:ValueReference>contact.creator-name</fes:ValueReference>
            <fes:Function name="template.value.v1">
                <fes:Literal/>
                <fes:Literal>my-id-same-source</fes:Literal>
                <fes:Literal>true</fes:Literal>
                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
        <fes:PropertyIsEqualTo matchAction="ANY" matchCase="false">
            <fes:ValueReference>contact.publisher-name</fes:ValueReference>
            <fes:Function name="template.value.v1">
                <fes:Literal/>
                <fes:Literal>my-id-same-source</fes:Literal>
                <fes:Literal>true</fes:Literal>
                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
        <fes:PropertyIsEqualTo matchAction="ANY" matchCase="false">
            <fes:ValueReference>contact.point-of-contact-name</fes:ValueReference>
            <fes:Function name="template.value.v1">
                <fes:Literal/>
                <fes:Literal>my-id-same-source</fes:Literal>
                <fes:Literal>true</fes:Literal>
                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
        <fes:PropertyIsEqualTo matchAction="ANY" matchCase="false">
            <fes:ValueReference>contact.contributor-name</fes:ValueReference>
            <fes:Function name="template.value.v1">
                <fes:Literal/>
                <fes:Literal>my-id-same-source</fes:Literal>
                <fes:Literal>true</fes:Literal>
                <fes:Literal>false</fes:Literal>
            </fes:Function>
        </fes:PropertyIsEqualTo>
    </fes:Or>
</fes:Filter>